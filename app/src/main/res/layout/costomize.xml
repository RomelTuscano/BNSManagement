<?xml version="1.0" encoding="utf-8"?>
<TableLayout
	android:id="@+id/widget33"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical"
	xmlns:android="http://schemas.android.com/apk/res/android">
<TextView
	android:id="@+id/widget39"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"

	android:layout_marginBottom="10dp"
	android:text="Select Table Name"
	android:textSize="20sp"
	android:typeface="sans"
	android:textStyle="bold"
	 />
<Spinner
	android:id="@+id/widget40"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
    android:entries="@array/acc_type"
    />
<TextView
	android:id="@+id/widget41"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="20dp"
	android:text="Select Attribute"
	android:textSize="20sp"
	android:typeface="sans"
	android:textStyle="bold"
	 />
<Spinner
	android:id="@+id/widget42"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="10dp"
    />
<EditText
	android:id="@+id/widget43"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="30dp"
	android:text=""
    android:hint="type here"
	android:textSize="18sp" />
<Button
	android:id="@+id/widget44"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginTop="30dp"
	android:text="SEARCH"
    android:onClick="findItFromDatabase" />

    <ScrollView
        android:layout_width="360dp"
        android:layout_height="180dp"
        android:id="@+id/cus_scrollView"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/edittext2"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp" />


    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/goBack_btn"
        android:textColor="#FFFFFF"
        android:background="@drawable/button_default_bg"
        android:id="@+id/goBackcus"
        android:onClick="goBack"/>



</TableLayout>
